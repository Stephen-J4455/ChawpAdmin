{
  "expo": {
    "name": "ChawpAdmin",
    "slug": "ChawpAdmin",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/chawp.png",
    "userInterfaceStyle": "dark",
    "platforms": ["ios", "android", "web"],
    "splash": {
      "image": "./assets/chawp.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": ["**/*"],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "notification": {
      "icon": "./assets/chawp.png",
      "color": "#FF6B35",
      "androidMode": "default"
    },
    "plugins": [
      "expo-system-ui",
      [
        "expo-notifications",
        {
          "icon": "./assets/chawp.png",
          "color": "#FF6B35"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 34,
            "buildToolsVersion": "35.0.0"
          }
        }
      ],
      "expo-font"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sj.chawpadmin"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/chawp.png",
        "backgroundColor": "#000000"
      },
      "package": "com.stephen_j.ChawpAdmin",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE"
      ]
    },
    "web": {
      "favicon": "./assets/chawp.png"
    },
    "extra": {
      "eas": {}
    }
  }
}
